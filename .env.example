# Environment variables for Traxxia Backend ML Application
# Copy this file to .env and replace the placeholder values with your actual API keys

# News API Key - Get from https://newsapi.org/
NEWSAPI_API_KEY=your_newsapi_key_here

# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your_openai_key_here

# Langfuse API Keys - Get from https://cloud.langfuse.com
LANGFUSE_SECRET_KEY=sk-lf-your_secret_key_here
LANGFUSE_PUBLIC_KEY=pk-lf-your_public_key_here
LANGFUSE_HOST=https://cloud.langfuse.com
